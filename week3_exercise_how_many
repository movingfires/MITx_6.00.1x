def how_many(aDict):
    '''
    aDict: A dictionary, where all the values are lists.

    returns: int, how many values are in the dictionary.
    '''
    total = 0
    for word in aDict:
        valuelist = []
        valuelist = aDict[word]
        for i in range(0, len(valuelist)):
            i += 1
        total += i
    return total
